{
  "name": "@kneelinghorse/semantic-protocol-vue",
  "version": "1.0.0",
  "description": "Vue 3 integration for Semantic Protocol - Universal meaning recognition for data",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "vite build && npm run build:types",
    "build:types": "vue-tsc --emitDeclarationOnly",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.vue",
    "lint:fix": "eslint src --ext .ts,.vue --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "semantic",
    "protocol",
    "composables",
    "directives",
    "typescript",
    "nuxt",
    "automatic-ui",
    "data-analysis",
    "ui-components"
  ],
  "author": "Semantic Protocol Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-protocol/semantic-protocol.git",
    "directory": "packages/vue"
  },
  "bugs": {
    "url": "https://github.com/semantic-protocol/semantic-protocol/issues"
  },
  "homepage": "https://github.com/semantic-protocol/semantic-protocol/tree/main/packages/vue#readme",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "dependencies": {
    "@kneelinghorse/semantic-protocol": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/kit": "^3.8.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.0.0",
    "pinia": "^2.1.0",
    "typescript": "^5.2.0",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}